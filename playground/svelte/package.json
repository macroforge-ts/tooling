{
	"name": "@playground/svelte",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "node --env-file=../../.env ./node_modules/.bin/vite dev",
		"build": "node --env-file=../../.env ./node_modules/.bin/vite build",
		"preview": "node --env-file=../../.env ./node_modules/.bin/vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@emotion/weak-memoize": "^0.4.0",
		"effect": "^3.19.4",
		"runed": "^0.36.0",
		"svelte-toolbelt": "^0.10.6",
		"ts-deepmerge": "^7.0.3"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "^2.6.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.10",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^25.0.3",
		"jsdom": "^26.1.0",
		"json-schema": "^0.4.0",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vitest": "^3.2.4"
	}
}
